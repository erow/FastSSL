    "version": "0.2.0",
    "configurations": [
        {
            "name": "Remote Attach",
            "type": "debugpy",
            "request": "attach",
            "connect": {
                "host": "nid010746",
                "port": 5679
            },
            "pathMappings": [
                {
                    "localRoot": "${workspaceFolder}",
                    "remoteRoot": "."
                }
            ]
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "SimCLR",
            "type": "debugpy",
            "request": "launch",
            "module": "torch.distributed.launch",
            "console": "integratedTerminal",
            "args": [
                "--nproc_per_node=1",
                "${workspaceFolder}/main_pretrain.py",
                "--batch_size=20",
                "--opt=adamw",
                "--blr=5e-4",
                "--epochs=100",
                "--data_path=../data/",
                "--data_set=cifar10",
                "--cfgs",
                "configs/cifar.gin",
                "configs/vitt.gin",
                "--gin",
                "build_dataset.transform_fn=@DataAugmentationDINO",
                "build_model.model_fn=@SimCLR",
                "build_model.embed_dim=192"
            ],
        },
        {
            "name": "VCL",
            "type": "debugpy",
            "request": "launch",
            "module": "torch.distributed.launch",
            "console": "integratedTerminal",
            "args": [
                "--master_port=12384",
                "${workspaceFolder}/main_pretrain.py",
                "--batch_size=500",
                "--opt=adamw",
                "--blr=5e-4",
                "--epochs=100",
                "--data_path=../data/",
                "--data_set=cifar10",
                "--cfgs",
                "configs/cifar.gin",
                "configs/vitt.gin",
                "--gin",
                "build_dataset.transform_fn=@DataAugmentationDINO",
                "build_model.model_fn=@VCL",
                "build_model.embed_dim=192"
            ],
        },
        {
            "name": "tmae",
            "type": "debugpy",
            "request": "launch",
            "module": "torch.distributed.launch",
            "console": "integratedTerminal",
            "args": [
                "--master_port=12384",
                "${workspaceFolder}/main_pretrain.py",
                "--batch_size=512",
                "--opt=adamw",
                "--blr=1e-4",
                "--epochs=100",
                "--num_workers=0",
                "--data_path=/high_perf_store3/l3_data/gent/data/IN1K",
                "--gin",
                "build_dataset.transform_fn=@SimpleAugmentation",
                "build_model.model_fn=@tmae_base",
            ],
        },
        {
            "name": "aim",
            "type": "debugpy",
            "request": "launch",
            "module": "torch.distributed.launch",
            "console": "integratedTerminal",
            "args": [
                "--nproc_per_node=1",
                "${workspaceFolder}/main_pretrain.py",
                "--batch_size=20",
                "--opt=adamw",
                "--blr=5e-4",
                "--epochs=100",
                "--data_path=../data/",
                "--data_set=cifar10",
                "--cfgs",
                "${workspaceFolder}/configs/cifar.gin",
                "--gin",
                "build_dataset.transform_fn=@SimpleAugmentation",
                "SimpleAugmentation.img_size=32",
                "build_model.model_fn=@aim_tiny",
            ],
        }
    ]
}